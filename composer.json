{
    "name": "lesha724/selfupdate",
    "description": "Basic extension for the Yii2 project self-update from VCS",
    "keywords": ["yii2", "selfupdate", "update", "deployment", "continuous", "integration", "VCS", "GIT", "Mercurial"],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Paul Klimov",
            "email": "klimov.paul@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6",
        "yiisoft/yii2": "~2.0.13"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {"yii2tech\\selfupdate\\": "src"}
    },
    "autoload-dev": {
        "psr-4": {
            "yii2tech\\tests\\unit\\selfupdate\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    }
}
